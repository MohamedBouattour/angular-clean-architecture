{
  "$schema": "http://json-schema.org/schema",
  "$id": "CleanSharedGenerator",
  "title": "Clean Shared Generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the shared asset",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "type": "string",
      "description": "Type of shared asset (defaults to 'all' if not specified)",
      "enum": ["all", "ui", "util"],
      "default": "all"
    }
  },
  "required": []
}
