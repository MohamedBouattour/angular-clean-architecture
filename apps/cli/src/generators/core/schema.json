{
  "$schema": "http://json-schema.org/schema",
  "$id": "CleanCoreGenerator",
  "title": "Clean Core Generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the core asset",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "type": "string",
      "description": "Type of core asset (defaults to 'all' if not specified)",
      "enum": [
        "all",
        "auth",
        "guard",
        "interceptor",
        "service",
        "translate",
        "language-selector",
        "theme-selector",
        "menu",
        "navbar"
      ],
      "default": "all"
    }
  },
  "required": []
}
