{
  "name": "angular-clean-architecture-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:feature": "nx g cli:feature",
    "generate:feature:interactive": "nx g cli:feature --interactive",
    "generate:core": "nx g cli:core --type=all",
    "generate:shared": "nx g cli:shared --type=all",
    "sandbox:clean": "node scripts/clean-sandbox.js",
    "sandbox:regenerate": "npm run sandbox:clean && npm run generate:core && npm run generate:shared && npm run generate:feature",
    "sandbox:reset": "npm run sandbox:regenerate && npm run sandbox:build",
    "cli:build": "nx build cli",
    "cli:publish": "node scripts/publish-cli.js",
    "cli:test": "nx test cli",
    "cli:lint": "nx lint cli",
    "sandbox:serve": "nx serve sandbox",
    "sandbox:build": "nx build sandbox",
    "sandbox:test": "nx test sandbox",
    "sandbox:lint": "nx lint sandbox",
    "all:lint": "nx run-many -t lint",
    "all:test": "nx run-many -t test",
    "all:build": "nx run-many -t build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "nx reset && rm -rf node_modules",
    "full-check": "npm run format:check && npm run all:lint && npm run all:test && npm run all:build"
  },
  "workspaces": [
    "apps/*"
  ],
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@devmed555/angular-clean-architecture-cli": "^0.0.2",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/signals": "^19.0.0",
    "@nx/devkit": "20.0.4",
    "@nx/nx-win32-x64-msvc": "20.0.4",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular-devkit/core": "^21.0.0",
    "@angular-devkit/schematics": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/language-service": "^21.0.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "20.0.4",
    "@nx/eslint": "20.0.4",
    "@nx/eslint-plugin": "20.0.4",
    "@nx/jest": "20.0.4",
    "@nx/js": "20.0.4",
    "@nx/plugin": "20.0.4",
    "@schematics/angular": "^21.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.0.0",
    "angular-eslint": "^19.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.1.0",
    "nx": "20.0.4",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.0.0"
  }
}
